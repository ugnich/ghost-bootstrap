{
    "name": "ghost-bootstrap",
    "description": "Free Ghost theme for SaaS using Bootstrap 5",
    "homepage": "https://github.com/ugnich/ghost-bootstrap",
    "version": "1.0.0",
    "license": "MIT",
    "engines": {
        "ghost": ">=3.0.0",
        "ghost-api": "v3"
    },
    "author": {
        "name": "ugnich",
        "url": "https://twitter.com/ugnich"
    },
    "contributors": {
        "name": "Enamul Haque",
        "url": "http://electronthemes-ghost.com/"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine",
            "Blog"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme",
        "bootstrap"
    ],
    "config": {
        "posts_per_page": 6,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    }
}
